#include <iostream>
#include <cmath>

using namespace std ;

int main() {
int m , n 
cout << "Enter the values of n and m " << endl;
cin >> n >> m ; 
double A[n][m] ;

for(int i = 0 ; i < n ; i++) {
   double sum[n][m] = 0.0 ;
    f or(int j = 0 ; j < m ; j++) {
          sum[i][j] += A[j][i]*A[i][j] 
   }

}
cout << "The sum of the values of A squared" << endl ;
for(int i = 0 ; i < n ; i++) {
    for(int j = 0 ; j < m ; j++) {
           cout << sum[i][j] << " " ;
    }
               cout <<  endl  <<  endl  ; 
}
return 0 ; 
}
